package edu.bonn.jbc.interfaces.task4;

public class StudentCopier {
	
	public Student[] copy(Student[] original) throws CloneNotSupportedException {
		Student[] copy = new Student[original.length];
		for (int i=0; i<original.length; i++) {
			copy[i] = (Student)original[i].clone();
		}
		return copy;
	}

}
